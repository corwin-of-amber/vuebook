{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "outDir": "static",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "jsx": "preserve"
  },
  "exclude": ["dist", "build"]
}